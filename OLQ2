  wget http://www.staff.hs-mittweida.de/~wuenschi/data/media/compbiolbook/chapter-11-regex--structure.pdb  
  
    #Change all Beisel to Weisel
  sed -e 's/Beisel/Weisel/' structure.pdb
  
